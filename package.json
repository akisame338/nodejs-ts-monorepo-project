{
  "scripts": {
    "dev": "concurrently 'npm run client:dev' 'npm run server:dev'",
    "client:dev": "vite --host",
    "server:dev": "nodemon --watch src/server --ext ts --exec 'ts-node --project tsconfig.server.json src/server/app'"
  },
  "dependencies": {
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.18",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "vite": "^4.4.9"
  }
}
